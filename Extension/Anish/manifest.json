{
  "name": "MakeSPP",
  "version": "1.0.8.14",
  "manifest_version": 2,
  "description": "MakeSPP",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": ["images/*.png","/images/*.png","*.png","template.html"],

  "browser_action": {
    "default_title": "ANISH!",
    "default_icon" : "images/anish.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
